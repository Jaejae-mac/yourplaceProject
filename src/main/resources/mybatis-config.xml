<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.yourplace.admin.login.vo.AdminVO"
			alias="myAdmin" />
		<typeAlias type="com.yourplace.admin.memberview.vo.MemberVO"
			alias="myMember" />
		<typeAlias type="com.yourplace.host.login.vo.HostVO"
			alias="myHost" />
		<typeAlias type="com.yourplace.custom.login.vo.UserVO"
			alias="myUser" />
		<typeAlias type="com.yourplace.custom.reserve.vo.ReserveVO"
			alias="reserveVO" />
			<typeAlias type="com.yourplace.custom.mypage.vo.MyPageCouponVO"
			alias="MyPageCouponVO"/>
		<typeAlias type="com.yourplace.host.reserv.vo.HostReservVO"
			alias="HostReservVO" />
		<typeAlias type="com.yourplace.host.review.vo.HostReviewVO"
			alias="HostReviewVO" />
		<typeAlias
			type="com.yourplace.host.management.vo.HostManagementVO"
			alias="HostManagementVO" />
		<typeAlias
			type="com.yourplace.host.management.vo.HostManagementRoomVO"
			alias="HostManagementRoomVO" />
		<typeAlias type="com.yourplace.custom.home.vo.PlaceCardVO"
			alias="cardPlace" />

		<typeAlias type="com.yourplace.host.regist.vo.PlaceVO"
			alias="myPlace" />
		<typeAlias type="com.yourplace.host.regist.vo.PlaceImgVO"
			alias="myPlaceImg" />
		<typeAlias type="com.yourplace.host.regist.vo.PlaceDetailVO"
			alias="myPlaceDetail" />
	</typeAliases>
	<mappers>
		<mapper resource="mappings/custom/home-cards-mapping.xml" />
		<mapper resource="mappings/test-mapper.xml" />
		<mapper resource="mappings/host/host-login-mapping.xml" />
		<mapper resource="mappings/host/regist-place-mapping.xml" />

		<mapper resource="mappings/custom/custom-login-mapping.xml" />
		<mapper resource="mappings/custom/custom-access-mapping.xml" />	
		<mapper resource="mappings/admin/admin-login-mapping.xml" />
		<mapper resource="mappings/admin/admin-allview-mapping.xml" />
		<mapper resource="mappings/custom/ReserveMapper.xml" />
		<mapper resource="mappings/custom/coupon-mapping.xml" />
		<mapper resource="mappings/host/HostReservMapper.xml" />
		<mapper resource="mappings/host/HostReviewMapper.xml" />
		<mapper resource="mappings/host/HostManagementMapper.xml" />
		<mapper resource="mappings/host/HostManagementRoomMapper.xml" />

	</mappers>
</configuration>