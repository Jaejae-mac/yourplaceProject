<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.yourplace.admin.login.vo.AdminVO"
			alias="myAdmin" />
		<typeAlias type="com.yourplace.admin.memberview.vo.MemberVO"
			alias="myMember" />
		<typeAlias type="com.yourplace.admin.coupon.all.vo.CouponAllVO"
			alias="myCouponAll" />
 		<typeAlias type="com.yourplace.admin.coupon.user.vo.CouponUserVO"
 			alias="myCouponUser" />
		<typeAlias type="com.yourplace.admin.space.vo.SpaceVO"
			alias="mySpaceAllow" />
		<typeAlias type="com.yourplace.admin.faq.guest.vo.FaqGuestVO"
			alias="myFAQGuest" />
		<typeAlias type="com.yourplace.admin.faq.host.vo.FaqHostVO"
			alias="myFAQHost" />
		<typeAlias type="com.yourplace.host.login.vo.HostVO"
			alias="myHost" />
		<typeAlias type="com.yourplace.custom.login.vo.UserVO"
			alias="myUser" />
		<typeAlias type="com.yourplace.custom.invoice.InvoiceVO"
			alias="myInvoice" />
		<typeAlias type="com.yourplace.custom.reserve.vo.ReserveVO"
			alias="reserveVO" />
		<typeAlias type="com.yourplace.host.reserv.vo.HostReservVO"
			alias="HostReservVO"/>

	</typeAliases>
	
	<mappers>
	
		<mapper resource="mappings/test-mapper.xml" />
		<mapper resource="mappings/host/host-login-mapping.xml" />
		<mapper resource="mappings/custom/custom-login-mapping.xml" />
		<mapper resource="mappings/custom/custom-invoice-mapping.xml" />
		<mapper resource="mappings/custom/custom-access-mapping.xml" />
		<mapper resource="mappings/admin/admin-login-mapping.xml" />
		<mapper resource="mappings/admin/admin-allview-mapping.xml" />
		<mapper resource="mappings/admin/admin-coupon-mapping.xml" />
		<mapper resource="mappings/admin/admin-coupon-user-mapping.xml" />
		<mapper resource="mappings/admin/admin-spaceallow-mapping.xml" />
		<mapper resource="mappings/admin/admin-faq-guest-mapping.xml" />
		<mapper resource="mappings/admin/admin-faq-host-mapping.xml" />
		<mapper resource="mappings/custom/ReserveMapper.xml" />
		<mapper resource="mappings/host/HostReservMapper.xml"/>

	</mappers>
</configuration>